    "def divide(a, b):\n",
    "  try: \n",
    "    result = a / b\n",
    "    print(\"The result is:\", result)\n",
    "  except ZeroDivisionError:\n",
    "    print(\"Numerator must be greater than zero.\")\n",
    "  except TypeError:\n",
    "    print(\"Inputs must be numbers.\")\n",
    "  else:\n",
    "    return result"
   ]
